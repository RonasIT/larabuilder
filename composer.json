{
    "name": "ronasit/larabuilder",
    "description": "A great tool for creating and modifying PHP classes in Laravel, providing utilities to generate boilerplate code, add methods, properties, and annotations with ease.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ronas IT",
            "homepage": "https://github.com/RonasIT/larabuilder/graphs/contributors"
        }
    ],
    "require": {
        "laravel/framework": "^12.28",
        "php": "^8.4"
    },
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "php-coveralls/php-coveralls": "^2.8",
        "phpunit/phpunit": "^12.3"
    },
    "autoload": {
        "psr-4": {
            "Ronasit\\Larabuilder\\": "src/"
        }
    }
}
